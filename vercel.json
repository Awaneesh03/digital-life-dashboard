{
    "headers": [
        {
            "source": "/assets/css/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        },
        {
            "source": "/assets/js/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}